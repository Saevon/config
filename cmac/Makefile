.PHONY: all
all:
	sudo bash -c "source ${PWD}/mac.sh"
	sudo ln -i -s "/Applications/Sublime Text 2.app/Contents/SharedSupport/bin/subl" /usr/local/bin/subl

	@echo ""
	@echo "Mac Customizations Complete"











