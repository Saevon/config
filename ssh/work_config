VisualHostKey yes


# Personal Repo Key
Host searea.github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github-searea
    IdentitiesOnly yes
    VisualHostKey no
Host saevon.github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github-saevon
    IdentitiesOnly yes
    VisualHostKey no
Host saevon.bitbucket.org
    User git
    IdentityFile ~/.ssh/bitbucket-saevon
    IdentitiesOnly yes
    VisualHostKey no


# Work Keys (Default for any other repo)
Host bitbucket.org
    IdentityFile ~/.ssh/bitbucket
    VisualHostKey no

Host github.com
    IdentityFile ~/.ssh/github
    VisualHostKey no


